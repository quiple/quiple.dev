---
import { getEntry } from 'astro:content'
import { Content as GalmuriContent } from '@/contents/galmuri.mdx'
import Layout from '@/layouts/Layout.astro'

const entry = await getEntry('galmuri')
const { Content } = await entry.render()
---

<Layout
