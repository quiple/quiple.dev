---
import {getCollection} from 'astro:content'
import Footer from '@/components/Footer.astro'
import Header from '@/components/Header.astro'
import {BlocksTable} from '@/components/unicode/blocks'
import Layout from '@/layouts/Layout.astro'

const unicodeBlocks = await getCollection('unicodeBlocks')
// const unicodeData = await getCollection('unicodeData')
---

<Layout title="유니코드 블록">
  <Header />
  <main class="container-x py-4">
    <!-- <BlocksTable client:load unicodeBlocks={unicodeBlocks} unicodeData={unicodeData} /> -->
    <BlocksTable client:load unicodeBlocks={unicodeBlocks} />
  </main>
  <Footer />
</Layout>
